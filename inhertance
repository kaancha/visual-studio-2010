using System;
namespace Inheritance
{
    class Animal
    {
        public String name;
        public void display()
        {
            Console.WriteLine("I am an Animal");
        }
    }
    class dog : Animal
    {
        public void getname()
        {
            Console.WriteLine("My name is " + name);
        }
    }
    class Program
    {
        static void Main(string[] args)
        {
            dog d1 = new dog();
            d1.name = "Sheru";
            d1.display();
            d1.getname();
            Console.ReadLine();
        }
    }
}
